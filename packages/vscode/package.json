{
  "name": "philodendron-insider",
  "displayName": "Philodendron - Insider",
  "description": "Test Playground",
  "version": "7.0.54",
  "license": "Apache-2.0",
  "engines": {
    "vscode": "^1.47.0"
  },
  "publisher": "Carmen",
  "categories": [
    "Programming Languages"
  ],
  "dependencies": {
    "vscode-extension-telemetry": "0.1.6",
    "checkpoint-client": "1.1.11",
    "crypto": "1.0.1",
    "chokidar": "3.4.2",
    "vscode-languageclient": "6.1.3",
    "test-philodendron-language-server": "7.0.52"
  },
  "optionalDependencies": {
    "fsevents": "2.1.3"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/prisma/vscode"
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "npm run build -- -w",
    "vscode:prepublish": "cd .. && cd .. && tsc --build --clean && npm run compile",
    "package": "vsce package"
  },
  "main": "./dist/src/extension.js",
  "activationEvents": [
    "onCommand:philodendron.helloWorld"
  ],
  "contributes": {
    "commands": [
      {
        "command": "philodendron.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/vscode": "1.47.0",
    "vsce": "1.77.0",
    "vscode-test": "1.4.0",
    "@types/mocha": "8.0.1",
    "mocha": "8.1.0",
    "semver": "7.3.2",
    "typescript": "3.9.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "preview": true
}
